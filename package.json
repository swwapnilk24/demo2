{
  "name": "spmapi",
  "version": "0.1.0",
  "main": "src",
  "config": {
    "app": {
      "name": "spmapi"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "scripts": {
    "start": "node .",
    "inspect": "node --debug-brk --inspect .",
    "debug": "node --debug src/index.js",
    "test": "jest",
    "test:unit": "npm test \"src/**/!(index).test.js\"",
    "test:integration": "npm test \"src/**/index.test.js\"",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html",
    "rest": "nodemon --debug=5860 server.js --exec babel-node --presets es2015,stage-1",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "gprecommit": "npm run lint",
    "commitmsg": "validate-commit-msg"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "apidoc-markdown": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "commitizen": "^2.9.6",
    "cross-env": "^3.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv-safe": "^4.0.3",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "jest-cli": "^18.1.0",
    "mockgoose": "^6.0.8",
    "moment": "^2.20.1",
    "nock": "^9.0.2",
    "nodemon": "^1.10.2",
    "opn-cli": "^3.1.0",
    "sinon": "^1.17.5",
    "standard-version": "^4.3.0",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "bodymen": "^1.0.1",
    "compression": "^1.6.2",
    "conventional-changelog": "^1.1.11",
    "cors": "^2.7.1",
    "dateformat": "^3.0.2",
    "emailjs": "^1.0.12",
    "es6-promise": "^4.0.5",
    "express": "^4.13.4",
    "express-force-ssl": "^0.3.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.4.19",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-create-unique": "^0.4.1",
    "mongoose-keywords": "^0.3.1",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "node-wit": "^4.3.0",
    "nodemailer": "^4.4.2",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^2.1.0",
    "passport-windowsauth": "^3.0.0",
    "querymen": "^2.0.0",
    "rand-token": "^0.3.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "sendgrid": "^4.0.2",
    "uuid-v4": "^0.1.0"
  },
  "bundledDependencies": [
    "apidoc",
    "apidoc-markdown",
    "babel-eslint",
    "babel-jest",
    "cross-env",
    "dotenv-safe",
    "eslint",
    "eslint-config-standard",
    "eslint-plugin-promise",
    "eslint-plugin-standard",
    "jest-cli",
    "mockgoose",
    "nock",
    "nodemon",
    "opn-cli",
    "sinon",
    "supertest",
    "supertest-as-promised",
    "babel-preset-es2015",
    "babel-register",
    "babel-core",
    "babel-plugin-transform-runtime",
    "babel-preset-es2015",
    "babel-preset-stage-1",
    "babel-runtime",
    "bcrypt",
    "bluebird",
    "body-parser",
    "bodymen",
    "compression",
    "cors",
    "es6-promise",
    "express",
    "express-force-ssl",
    "isomorphic-fetch",
    "jsonwebtoken",
    "lodash",
    "mongoose",
    "mongoose-create-unique",
    "mongoose-keywords",
    "morgan",
    "node-wit",
    "passport",
    "passport-http",
    "passport-http-bearer",
    "passport-jwt",
    "querymen",
    "rand-token",
    "request",
    "request-promise",
    "sendgrid",
    "nodemailer",
    "uuid-v4"
  ]
}
